{
  "name": "lazzard/php-ftp-client",
  "description": "A library that wraps the PHP FTP functions in an OOP way.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "php",
    "ftp-library",
    "ftp-client",
    "php-ftp-client"
  ],
  "authors": [
    {
      "name": "El Amrani Shaker",
      "email": "elamrani.sv.laza@gmail.com",
      "homepage": "https://github.com/AmraniCh"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "ext-ftp": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5"
  },
  "autoload": {
    "psr-4": {
      "Lazzard\\FtpClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lazzard\\FtpClient\\Tests\\": "tests/"
    }
  }
}
